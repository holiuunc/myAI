{
  "functions": {
    "api/documents/process/*": {
      "memory": 2048,
      "maxDuration": 60
    },
    "api/**/*.ts": {
      "memory": 2048,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next"
} 